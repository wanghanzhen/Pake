    "prompts": "^2.4.2",
    "tmp-promise": "^3.0.3"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.0.2",
    "@types/is-url": "^1.2.30",
    "@types/page-icon": "^0.3.4",
